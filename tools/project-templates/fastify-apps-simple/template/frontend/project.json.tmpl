{
  "name": "@internal/{{APP_NAME}}-frontend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "fastify-apps/{{APP_NAME}}/frontend/src",
  "projectType": "application",
  "tags": [
    "type:frontend",
    "scope:fastify"
  ],
  "targets": {
    "lint": {},
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "npm run build",
        "cwd": "fastify-apps/{{APP_NAME}}/frontend"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "fastify-apps/{{APP_NAME}}/frontend"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run preview",
        "cwd": "fastify-apps/{{APP_NAME}}/frontend"
      }
    }
  }
}
