{
  "name": "@internal/fastify-{{APP_NAME}}",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "fastify-apps/{{APP_NAME}}/src",
  "projectType": "application",
  "tags": [
    "type:app",
    "scope:fastify"
  ],
  "implicitDependencies": [
    "@internal/skeleton"
  ],
  "targets": {
    "lint": {},
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run app:dev",
        "cwd": "fastify-apps/{{APP_NAME}}"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run app:start",
        "cwd": "fastify-apps/{{APP_NAME}}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "fastify-apps/{{APP_NAME}}"
      }
    }
  }
}
