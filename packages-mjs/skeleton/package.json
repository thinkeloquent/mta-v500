{
  "name": "@internal/skeleton",
  "version": "1.0.8",
  "private": false,
  "description": "A lightweight Fastify skeleton application with TypeScript support for building fast and scalable web applications",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/self-launch-server.ts",
    "dev:watch": "tsx watch src/self-launch-server.ts",
    "build": "tsc",
    "start": "node ./dist/self-launch-server.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "fastify",
    "typescript",
    "skeleton",
    "boilerplate",
    "web-framework",
    "rest-api",
    "nodejs",
    "backend"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mta-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mta-skeleton/issues"
  },
  "homepage": "https://github.com/your-username/mta-skeleton#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/etag": "^6.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.0.2",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@internal/core-exceptions": "workspace:*",
    "@internal/core-no-cache": "workspace:*",
    "@internal/core-plugin-logger": "workspace:*",
    "@internal/core-prisma-utils": "workspace:*",
    "@internal/core-route-logger": "workspace:*",
    "@internal/core-static-app": "workspace:*",
    "close-with-grace": "^2.1.0",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
